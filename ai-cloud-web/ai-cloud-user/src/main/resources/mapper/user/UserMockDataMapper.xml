<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.user.mapper.UserMockDataMapper">
    
    <resultMap type="com.cloud.user.domain.UserMockData" id="UserMockDataResult">
        <result property="id" column="id"/>
        <result property="type" column="type"/>
        <result property="value" column="value"/>
        <result property="statusFlag" column="status_flag"/>
        <result property="deleteFlag" column="delete_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>
    
    <select id="selectByMockData" resultType="com.cloud.user.domain.UserMockData">
        SELECT id,
               type,
               `value`
        FROM user_mock_data
        WHERE type = #{type}
          AND status_flag = 0
          AND delete_flag = 0
    </select>


</mapper>